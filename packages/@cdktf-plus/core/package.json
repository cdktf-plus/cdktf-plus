{
  "name": "@cdktf-plus/core",
  "version": "0.0.0",
  "description": "A collection of helpful L2 / L3 constructs for cdktf",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "jsii --silence-warnings reserved-word",
    "package": "jsii-pacmak",
    "package:js": "jsii-pacmak --targets js",
    "dist-clean": "rm -rf dist"
  },
  "author": "Sebastian Korfmann",
  "license": "MPL-2.0",
  "homepage": "https://github.com/cdktf-plus/cdktf-plus",
  "repository": {
    "type": "git",
    "url": "https://github.com/cdktf-plus/cdktf-plus.git",
    "directory": "packages/@cdktf-plus/core"
  },
  "peerDependencies": {
    "@cdktf/provider-external": "^0.4.31",
    "@cdktf/provider-null": "^0.5.31",
    "cdktf": "^0.9.2",
    "constructs": "^10.0.71"
  },
  "devDependencies": {
    "@cdktf/provider-external": "^0.4.31",
    "@cdktf/provider-null": "^0.5.31",
    "@types/node": "^16.3.1",
    "cdktf": "^0.9.2",
    "constructs": "^10.0.71",
    "jsii": "^1.54.0",
    "jsii-pacmak": "^1.54.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@cdktf/provider-external": "^0.4.31",
    "@cdktf/provider-null": "^0.5.31",
    "docker-registry-client": "^3.3.0",
    "esbuild-wasm": "^0.14.23",
    "execa": "^5.1.1",
    "hashdirectory": "^0.1.0"
  },
  "bundledDependencies": [
    "hashdirectory",
    "docker-registry-client",
    "esbuild-wasm",
    "execa"
  ],
  "jsii": {
    "outdir": "dist",
    "versionFormat": "short",
    "targets": {
      "python": {
        "distName": "cdktf-plus-core",
        "module": "cdktf_plus_core"
      }
    }
  }
}
